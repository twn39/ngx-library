/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "paths": {
      "ngx-simple-i18n": [
        "dist/ngx-simple-i18n/ngx-simple-i18n",
        "dist/ngx-simple-i18n"
      ],
      "ngx-simple-popper": [
        "dist/ngx-simple-popper/ngx-simple-popper",
        "dist/ngx-simple-popper"
      ],
      "ngx-simple-qrcode": [
        "dist/ngx-simple-qrcode/ngx-simple-qrcode",
        "dist/ngx-simple-qrcode"
      ],
      "ngx-vconsole-logger": [
        "dist/ngx-vconsole-logger/ngx-vconsole-logger",
        "dist/ngx-vconsole-logger"
      ]
    },
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2017",
    "module": "es2020",
    "lib": [
      "es2020",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
